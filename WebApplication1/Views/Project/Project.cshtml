@model WebApplication1.Models.ProjectDetailViewModel

@{
    ViewBag.Title = Model.Name;
}

<h2><i class="fa fa-address-card icon" aria-hidden="true"></i> @Model.Name</h2>
<div class="row">
    <div class="col-md-3">
        <div class="panel panel-default ">
            <div class="panel-heading">
                <h3 class="panel-title">@Project.ProjectOwner</h3>
            </div>
            <div class="panel-body">
                @Model.OwnerName
            </div>
        </div>
    </div>
    <div class="col-md-3">
        <div class="panel panel-default ">
            <div class="panel-heading">
                <h3 class="panel-title">@Project.ClientName</h3>
            </div>
            <div class="panel-body">
                @Model.ClientName
            </div>
        </div>
    </div>
</div>